package com.tms.dsa.sorting;

import java.util.Random;

public class QuickSort {

	public static void main(String[] args) {

	}

	public static int[] generateNumberArray(int size) {
		int[] arr = new int[size];
		Random r = new Random();
		for (int i = 0; i < size; ++i) {
			arr[i] = r.nextInt(100);
		}
		return arr;
	}

}
